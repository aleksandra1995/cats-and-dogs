<%= form_for @dog do |f| %>

    <%= f.label :name %>
    <%= f.text_field :name %>
    
    <%= f.label :age %>
    <%= f.text_field :age %>
    
    <%= f.label :city %>
    <%= f.text_field :city %>
    
    <%= f.label :breed %>
    <%= f.text_field :breed %>
    
    <%= f.label :bio %>
    <%= f.text_field :bio %>

    <%= f.label :temperament %>
    <%= f.text_field :temperament %>

    <%= f.label :picture %>
    <%= f.text_field :picture %>

    
    <%= f.submit %>
    <% end %>